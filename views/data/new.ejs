<body>
    <div class="row">
        <h1 class="text-center">Add New Data</h1>
        <div class="col-6 offset-3">
            <form action="/campgrounds" method="POST" novalidate class="validated-form">
                <!-- <div class="mb-3">
                    <label class="form-label" for="sr">Sr.</label>
                    <input class="form-control" type="number" id="sr" name="" required>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div> -->
                <div class="mb-3">
                    <label class="form-label" for="particular">Particulars</label>
                    <input class="form-control" type="text" id="particular" name="" required>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                <div class="mb-3">
                    <label class="form-label" for="date">Date</label>
                    <input class="form-control" type="date" id="" name="" required>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                <div class="mb-3">
                    <label class="form-label" for="lastdate"> Last Date</label>
                    <input class="form-control" type="date" id="" name="" required>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                <div class="mb-3">
                    <label class="form-label" for="image">Image Url</label>
                    <input class="form-control" type="url" id="image" name="" required>
                    <div class="valid-feedback">
                        Looks good!
                    </div>
                </div>
                <div class="mb-3">
                    <button class="btn btn-success">Add New Data</button>
                </div>
            </form>
            <a href="./admin"> Back to Admin Page </a>
        </div>
    </div>



    
